{
  "name": "@visualon/camljs",
  "version": "2.8.2",
  "description": "Simplifies creation of SharePoint CAML queries for client-side scripts. Can be used with either SharePoint Client Object Model or SPServices.",
  "main": "./dist/camljs.js",
  "types": "./dist/camljs.d.ts",
  "scripts": {
    "start": "tsc -w --pretty",
